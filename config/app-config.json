{
  "connection": {
    "url": "ws://192.168.1.220:8765"
  },
  "pointcloud": {
    "topic": "/cloud_registered",
    "decayTimeMs": 4000,
    "maxPoints": 100000,
    "pointSize": 2
  },
  "camera": {
    "topic": "/usb_cam/image_raw"
  },
  "stats": {
    "topic": "/supervisor/monitor/system"
  },
  "nodes": {
    "topic": "/supervisor/monitor/nodes",
    "startService": "/supervisor/actions/start_node",
    "stopService": "/supervisor/actions/stop_node",
    "exclude": ["/rosout", "/foxglove_nodelet_manager"],
    "launch": [
      {
        "package": "fast_lio",
        "launchFile": "mapping_ouster64_jetson.launch",
        "args": [
          {
            "key": "rviz",
            "value": "false"
          }
        ]
      },
      {
        "package": "ouster_ros",
        "launchFile": "driver.launch",
        "args": [
          {
            "key": "sensor_hostname",
            "value": "192.168.100.2"
          },
          {
            "key": "viz",
            "value": "false"
          }
        ]
      }
    ]
  },
  "recorder": {
    "topic": "/supervisor/monitor/records",
    "statusTopic": "/supervisor/monitor/recording",
    "deleteService": "/supervisor/actions/delete_recording",
    "startService": "/supervisor/actions/start_recording",
    "stopService": "/supervisor/actions/stop_recording",
    "topics": [
      "/ouster/points"
    ]
  }
}
